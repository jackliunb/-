//第一题
fn main() {
    let light = TrafficLight::Red;
    println!("light is: {}", light.time());
}

enum TrafficLight {
    Red,
    Green,
    Yellow,
}

impl TrafficLight {
    fn time(&self) -> u8{
        60
    }
}


//第三题
struct Rectangle {
    width: u32,
    length: u32,
}

fn main() {
    let rect = Rectangle {
        width: 30,
        length: 50,
    };
    println!("{}",area(&rect));
}

fn area(rect: &Rectangle) -> u32 {
    rect.width * rect.length
}
